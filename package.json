{
  "name": "fullstackcookbook",
  "version": "1.0.0",
  "description": "Cook book site for users to store their own recipes",
  "main": "server.js",
  "scripts": {
    "start": "concurrently --kill-others \"node backend/server.js\" \"cd ./frontend/ && npm run start\"",
    "build": "cd backend && npm install && cd ../frontend && npm install && npm run build",
    "server": "nodemon backend/server.js"
  },
