{
  "name": "fullstackcookbook",
  "version": "1.0.0",
  "proxy": "http://localhost:5000",
  "description": "Cook book site for users to store their own recipes",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all -p server client",
    "client": "cd ./frontend && npm run start",
    "server": "node backend/server.js",
    "build": "(npm install) && (cd frontend && npm install && npm run build)",
    "devServer": "nodemon backend/server.js"
  },
  "keywords": [],
  "author": "Erik Galindo",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5"
  }
}
